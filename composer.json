{
    "name": "pieterf/laravel-ing-ideal-advanced",
    "description": "ING iDeal advanced API client wrapper for Laravel",
    "keywords": [
        "pieterf",
        "laravel",
        "laravel-ing-ideal-advanced",
        "ideal",
        "ing"
    ],
    "homepage": "https://github.com/pieterf/laravel-ing-ideal-advanced",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Floor",
            "email": "pieterfloor@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Pieterf\\LaravelIngIdealAdvanced\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pieterf\\LaravelIngIdealAdvanced\\LaravelIngIdealAdvancedServiceProvider"
            ],
            "aliases": {
                "LaravelIngIdealAdvanced": "Pieterf\\LaravelIngIdealAdvanced\\Facades\\LaravelIngIdealAdvanced"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
